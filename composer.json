{
    "name": "valentiniljaz/packagist-publish",
    "version": "0.1.0",
    "description": "Publish your package to Packagist.com using API. Very useful for PHP monorepos.",
    "homepage": "https://github.com/valentiniljaz/packagist-publish",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Valentin IljazÌŒ",
            "email": "valentin@progmbh.com"
        }
    ],
    "minimum-stability": "dev",
    "bin": [
        "bin/publish", "bin/archive"
    ],
    "require": {
        "php": ">=7.4",
        "splitbrain/php-cli": "dev-master",
        "ext-json": "*",
        "symfony/process": "6.1.x-dev",
        "guzzlehttp/guzzle": "7.4.x-dev",
        "ext-fileinfo": "*"
    },
    "autoload": {
        "psr-4": {
            "VI\\PackagistPublish\\": "src/"
        }
    }
}
